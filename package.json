{
  "name": "@mojolabs-id/ts-module-skeleton",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Mojolabs <root@mojolabs.id>",
  "license": "MIT",
  "type": "commonjs",
  "private": true,
  "packageManager": "yarn@3.3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojolabs-id/ts-module-skeleton.git"
  },
  "bugs": {
    "url": "https://github.com/mojolabs-id/ts-module-skeleton/issues"
  },
  "homepage": "https://github.com/mojolabs-id/ts-module-skeleton#readme",
  "scripts": {
    "start": "node dist/demo/test.js",
    "build": "tsup",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "files": [
    "/dist"
  ],
  "jest": {
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "node_modules",
      "<rootDir>"
    ],
    "collectCoverage": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.[tj]s$"
  },
  "dependencies": {

  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "cross-env": "^7.0.3",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.10",
    "typescript": "^4.9.5"
  }
}
